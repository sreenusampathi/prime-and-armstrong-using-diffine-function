 #include<stdio.h>
void checkprime ()
{
	int i,n,count=0;
	printf("enter any number");
	scanf("%d",&n);
	for(i=1;i<=n;i++)
	{
		if(n%i==0)
		{
			count++;
		}
	}
	if(count==2)
	{
		printf("the entered number is prime number");
	}
	else
	{
		printf("the entered number is not a prime number");
	}
	
}
void main()
{
	checkprime();
	  getch();
}
